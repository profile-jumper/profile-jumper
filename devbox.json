{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.0/.schema/devbox.schema.json",
  "packages": [
    "nodejs@23.9.0"
  ],
  "env": {
    "DEVBOX_COREPACK_ENABLED": "true"
  },
  "shell": {
    "init_hook": [
      "alias code.run='npm run start'",
      "alias code.build='npm run build'",
      "alias code.preview='npm run preview'",
      "alias deps.clean='./scripts/clean-deps.sh'",
      "alias deps.install='npm install'",
      "npm install"
    ],
    "scripts": {
      "test": [
        "echo \"Error: no test specified\" && exit 1"
      ]
    }
  }
}
